#: src/misc.c:310
#, c-format
msgid "tunnel to somewhere:   %s -L hostname:port -p port [options]\n"
msgstr "modo tunelamento:    %s -L hostname:porta -p porta [opções]\n"

#: src/misc.c:312
msgid ""
"Mandatory arguments to long options are mandatory for short options too.\n"
msgstr ""
"Os argumentos obrigatórios para as opções longas são obrigatórios para "
"as curtas também.\n"

#: src/misc.c:313
msgid ""
"Options:\n"
"  -c, --close                close connection on EOF from stdin\n"
"  -e, --exec=PROGRAM         program to exec after connect\n"
"  -g, --gateway=LIST         source-routing hop point[s], up to 8\n"
"  -G, --pointer=NUM          source-routing pointer: 4, 8, 12, ...\n"
"  -h, --help                 display this help and exit\n"
"  -i, --interval=SECS        delay interval for lines sent, ports scanned\n"
"  -l, --listen               listen mode, for inbound connects\n"
msgstr ""
"Opções:\n"
"  -c, --close                encerra a conexão ao EOF do stdin\n"
"  -e, --exec=PROGRAM         programa a executar depois de se conectar\n"
"  -g, --gateway=LIST         source-routing hop point[s], até a 8\n"
"  -G, --pointer=NUM          source-routing pointer: 4, 8, 12, ...\n"
"  -h, --help                 mostra este texto e sai\n"
"  -i, --interval=SECS        intervalo para as linhas enviadas ou portas\n"
"                             tentadas.\n"
"  -l, --listen               modo de escuta, para as conexões de entrada\n"

#: src/misc.c:321
msgid ""
"  -L, --tunnel=ADDRESS:PORT  forward local port to remote address\n"
"  -n, --dont-resolve         numeric-only IP addresses, no DNS\n"
"  -o, --output=FILE          output hexdump traffic to FILE (implies -x)\n"
"  -p, --local-port=NUM       local port number\n"
"  -r, --randomize            randomize local and remote ports\n"
"  -s, --source=ADDRESS       local source address (ip or hostname)\n"
msgstr ""
"  -L, --tunnel=ADDRESS:PORT  repassa uma porta a um endereço remoto\n"
"  -n, --dont-resolve         apenas endereços IP numéricos, sem DNS\n"
"  -o, --output=FILE          escrive a saída do tráfego em FILE (implica -x)\n"
"  -p, --local-port=NUM       especifica a porta local usada\n"
"  -r, --randomize            aleatoriza as portas locais e remotas\n"
"  -s, --source=ADDRESS       especifica o endereço de destino (ip ou hostname)\n"

#: src/misc.c:329
msgid ""
"  -t, --tcp                  TCP mode (default)\n"
"  -T, --telnet               answer using TELNET negotiation\n"
msgstr ""
"  -t, --tcp                  modo TCP (padrão)\n"
"  -T, --telnet               responde às negociações TELNET\n"

#: src/misc.c:333
msgid ""
"      --tcp                  TCP mode (default)\n"
"  -t, --telnet               answer using TELNET negotiation\n"
"  -T                         same as --telnet (compat)\n"
msgstr ""
"      --tcp                  modo TCP (padrão)\n"
"  -t, --telnet               responde às negociações TELNET\n"
"  -T                         igual a --telnet (compatibilidade)\n"

#: src/misc.c:338
msgid ""
"  -u, --udp                  UDP mode\n"
"  -v, --verbose              verbose (use twice to be more verbose)\n"
"  -V, --version              output version information and exit\n"
"  -x, --hexdump              hexdump incoming and outgoing traffic\n"
"  -w, --wait=SECS            timeout for connects and final net reads\n"
"  -z, --zero                 zero-I/O mode (used for scanning)\n"
msgstr ""
"  -u, --udp                  modo UDP\n"
"  -v, --verbose              detalhado (use duas vezes para mais detalhes)\n"
"  -V, --version              mostra informações sobre a versão e sai\n"
"  -x, --hexdump              mostra o hexdump dos dados na entrata e na saída\n"
"  -w, --wait=SECS            tempo de timeout para as tentativas de conexões\n"
"  -z, --zero                 modo de zero-I/O (usado para scanning)\n"

#: src/misc.c:346
msgid "Remote port number can also be specified as range.  Example: '1-1024'\n"
msgstr "A porta remota pode ser especificada como intervalo.  Exemplo: '1-1024'\n"

